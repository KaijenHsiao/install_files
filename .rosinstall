- other: {local-name: /opt/ros/groovy/share/ros}
- other: {local-name: /opt/ros/groovy/share}
- other: {local-name: /opt/ros/groovy/stacks}
- setup-file: {local-name: /opt/ros/groovy/setup.sh}

# interactive manipulation source code from github
- git: {local-name: src/object_manipulation, uri: 'git@github.com:ros-interactive-manipulation/object_manipulation.git', version: groovy-devel}
- git: {local-name: src/pr2_object_manipulation, uri: 'git@github.com:ros-interactive-manipulation/pr2_object_manipulation.git', version: groovy-devel}

# interactive markers
# - git: {local-name: src/interactive_markers, uri: 'https://github.com/ros-visualization/interactive_markers.git', version: groovy-devel}

# pr2 plugs
- hg: {local-name: src/pr2_plugs, uri: 'https://kforge.ros.org/plugs/hg'}
- hg: {local-name: src/smach, uri: 'https://kforge.ros.org/smach/executive_smach'}

# web interface dependencies
- git: {local-name: src/interactive-manipulation-sandbox, uri: 'git@github.com:willowgarage/interactive-manipulation-sandbox.git', version: groovy-devel}
- git: {local-name: src/rosbridge_suite, uri: 'git://github.com/RobotWebTools/rosbridge_suite.git', version: fuerte-devel}
- svn: {local-name: src/mjpeg_server, uri: 'http://svn.code.sf.net/p/bosch-ros-pkg/code/trunk/stacks/mjpeg_server'}
- git: {local-name: src/ros_http_video_streamer, uri: 'git@github.com:ros-interactive-manipulation/ros_http_video_streamer.git', version: master}
- git: {local-name: src/interactive_marker_proxy, uri: 'git@github.com:dgossow/interactive_marker_proxy.git', version: groovy-devel}
- git: {local-name: src/tf2_web_republisher, uri: 'git@github.com:RobotWebTools/tf2_web_republisher.git'}

# people tracking and trunk opencv. if you don't want to track people, you can remove this
